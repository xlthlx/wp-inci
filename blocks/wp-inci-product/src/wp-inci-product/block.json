{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wp-inci/product",
	"version": "0.1.0",
	"title": "WP INCI Product",
	"category": "text",
	"description": "Show a Product into the content of any post type.",
	"keywords": [ "product", "wp-inci" ],
	"supports": {
		"html": false,
		"customClassName": false
	},
	"attributes": {
		"productId": {
			"type": "string",
			"default": "0"
		},
		"productLink": {
			"type": "string"
		},
		"productContent": {
			"type": "string"
		},
		"customTitle": {
			"type": "string"
		},
		"ingredientsList": {
			"type": "string"
		},
		"ingredientsSafety": {
			"type": "string"
		},
		"disclaimer": {
			"type": "string"
		}
	},
	"textdomain": "wp-inci",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./../../../../public/css/wp-inci.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
